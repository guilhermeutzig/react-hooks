{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose up dev",
    "build": "docker-compose up build",
    "svg-to-react": "docker-compose up vectors",
    "lint-staged": "lint-staged",
    "lintjs": "eslint -c /app/.eslintrc /app/src/**/* -f checkstyle -o eslint.xml",
    "lintcss": "stylelint --config /app/.stylelintrc /app/src/**/*.scss",
    "lintjs-local": "eslint -c .eslintrc src/**/*",
    "lintcss-local": "stylelint --config .stylelintrc src/**/*.scss",
    "start": "npm run build && npm run server",
    "server": "node server/server.js",
    "build-prod": "/app/_extras/build/build-prod.sh",
    "docker-dev": "NODE_ENV=development webpack-dev-server --mode development --config ./config/webpack.config.js",
    "docker-build": "env NODE_ENV=production webpack --mode production --config ./config/webpack.config.js",
    "docker-vectors": "svgr --no-prettier -d src/structure/icons assets/svg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "linters": {
      "src/**/*.scss": [
        "stylelint --syntax=scss",
        "git add"
      ],
      "src/**/*.js": [
        "eslint",
        "git add"
      ],
      "src/**/*.jsx": [
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "src/structure/icons/*.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-3": "7.0.0",
    "@svgr/cli": "^4.1.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-react-docgen": "2.0.0",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "file-loader": "3.0.1",
    "history": "4.7.2",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "3.2.0",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "node-sass-utils": "^1.1.2",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.16.0",
    "prop-types": "15.6.2",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-hot-loader": "4.6.3",
    "react-loadable": "5.5.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "sass-loader": "7.1.0",
    "sass-mediaqueries": "1.6.1",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "0.8.1",
    "terser-webpack-plugin": "^1.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "^1.7.3",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "fs-extra": "^7.0.1",
    "helmet": "3.12.1"
  }
}
